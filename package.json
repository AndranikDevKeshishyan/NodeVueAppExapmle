{
  "name": "NodeVueAppExapmle",
  "version": "0.1.0",
  "title": "Node Vue example with advanced node contailner",
  "description": "Express Node api with Mongo and VueJS front",
  "main": "server/index.js",
  "scripts": {
    "dev": "cross-env NODE_ENV=development nodemon --inspect",
    "start": "cross-env NODE_ENV=production node server/index.js",
    "start:cluster": "cross-env NODE_ENV=production node cluster.js",
    "test": "npm run test:unit",
    "lint": "eslint --ext=.js,.server",
    "lint:fix": "eslint --fix --ext=.js,.server"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:AndranikDevKeshishyan/NodeVueAppExapmle.git"
  },
  "keywords": [
    "express",
    "mongo",
    "vue"
  ],
  "config": {
    "dbName": "nodeVueApp"
  },
  "author": "Andranik Keshishyan",
  "license": "MIT",
  "dependencies": {
    "body-parser": "1.18.2",
    "chalk": "1.1.3",
    "clui": "^0.3.6",
    "compression": "1.7.1",
    "cross-env": "5.0.0",
    "express": "4.16.0",
    "express-list-endpoints": "3.0.0",
    "express-status-monitor": "1.0.0",
    "express-validator": "3.2.0",
    "helmet": "3.8.2",
    "helmet-crossdomain": "0.1.0",
    "lodash": "4.17.5",
    "method-override": "2.3.10",
    "moment": "2.19.3",
    "mongoose": "4.10.2",
    "mongoose-auto-increment": "5.0.1",
    "morgan": "1.9.0",
    "pretty-bytes": "^5.1.0",
    "uuid-token-generator": "^1.0.0",
    "winston": "2.3.1",
    "winston-daily-rotate-file": "1.4.6"
  },
  "devDependencies": {
    "cross-env": "^5.0.0",
    "cross-spawn": "5.1.0",
    "eslint": "3.19.0",
    "eslint-friendly-formatter": "3.0.0",
    "eslint-loader": "1.7.1",
    "eslint-plugin-html": "2.0.3",
    "mocha": "3.4.2",
    "nodemon": "^1.11.0"
  }
